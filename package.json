{
  "name": "@webformula/material",
  "version": "0.7.0",
  "description": "Material web components. Performant, light-weight, full features, optimized for multiple devices.",
  "main": "src/index.js",
  "type": "module",
  "exports": {
    ".": "src/index.js",
    "./styles": "src/styles.js"
  },
  "scripts": {
    "start": "webpack serve --open --mode development",
    "start-production": "NODE_ENV=production webpack serve --open --mode production",
    "start-no-open": "webpack serve --mode development",
    "build:components": "NODE_ENV=production webpack --mode production --config ./webpack.bundle.config.js",
    "build:docs": "NODE_ENV=production webpack --mode production",
    "build": "npm run build:components & npm run build:docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webformula/material.git"
  },
  "author": "Ben Rubin (B-3PO)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/webformula/material/issues"
  },
  "homepage": "https://github.com/webformula/material#readme",
  "devDependencies": {
    "@webformula/core": "^0.7.1",
    "@webformula/material": "file:./",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
